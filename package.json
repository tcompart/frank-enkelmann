    "npm-run-all": "^4.1.1",
    "prop-types": "latest",
    "react": "16.0.0",
    "react-app-polyfill": "1.0.3",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-icons": "^2.2.7",
    "react-phone-number": "^1.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "4.2.2",
